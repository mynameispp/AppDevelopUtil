<?xml version="1.0" encoding="utf-8"?>
<com.ffzxnet.developutil.utils.ui.RoundLinerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:backgroundColor="@color/white_F5"
    app:round_corner="6dp"
    app:stroke_color="@color/colorPrimary"
    app:stroke_width="1dp">

    <TextView
        android:id="@+id/item_anthology_video_num"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:minEms="5"
        android:maxEms="5"
        android:singleLine="true"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp" />
</com.ffzxnet.developutil.utils.ui.RoundLinerLayout>